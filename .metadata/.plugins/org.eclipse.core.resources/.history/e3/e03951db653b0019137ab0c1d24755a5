package br.com.fiap.entity;

import java.util.Calendar;

/**
 * T_CURSO(SQ_T_CURSO)
 *		*cd_curso(NUMBER) (PK)
 * 		*nm_curso(VARCHAR(15))
 * 		*dt-lancamento(DATE)
 * 		ds_categoria(VARCHAR)(Enum: TECNICO, BACHAREL, POS)
 */

public class Curso {
	
	private int codigo;
	
	private String nome;
	
	private Calendar dataLancamento;
	
	private Categoria descricao;
}
